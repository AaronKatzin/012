<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->



    <style name="Theme.012" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">#FFFFFF</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/menu_background</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/fuzzybubbles</item>
    </style>

    <style name="Theme.012.GridColorTheme1" parent="Theme.012">
        <item name="colorPrimaryContainer">@color/theme_1_tileZero</item>
        <item name="colorSecondaryContainer">@color/theme_1_tileOne</item>
    </style>
    <style name="Theme.012.GridColorTheme2" parent="Theme.012">
        <item name="colorPrimaryContainer">@color/theme_2_tileZero</item>
        <item name="colorSecondaryContainer">@color/theme_2_tileOne</item>
        <item name="android:textColor"></item>
    </style>
</resources>